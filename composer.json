{
    "name": "archlinux-de/forum.archlinux.de",
    "description": "The German Arch Linux forum",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.0",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "archlinux-de/flarum-anti-spam": "^1.1.1",
        "archlinux-de/flarum-import-fluxbb": "^1.1.5",
        "archlinux-de/flarum-redirect-fluxbb": "^1.0.2",
        "archlinux-de/flarum-redirect-ll": "^1.0.4",
        "archlinux-de/flarum-theme-archlinux": "^1.0.2",
        "flarum/approval": "^1.0.0",
        "flarum/bbcode": "^1.0.0",
        "flarum/core": "^1.0.0",
        "flarum/emoji": "^1.0.0",
        "flarum/flags": "^1.0.0",
        "flarum/lang-english": "^1.0.0",
        "flarum/likes": "^1.0.0",
        "flarum/lock": "^1.0.0",
        "flarum/markdown": "^1.0.0",
        "flarum/mentions": "^1.0.0",
        "flarum/nicknames": "^1.0.0",
        "flarum/statistics": "^1.0.0",
        "flarum/sticky": "^1.0.0",
        "flarum/subscriptions": "^1.0.0",
        "flarum/suspend": "^1.0.0",
        "flarum/tags": "^1.0.0",
        "migratetoflarum/old-passwords": "^1.0.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^0.12.88",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "squizlabs/php_codesniffer": "^3.6.0"
    }
}
