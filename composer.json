{
    "name": "archlinux-de/forum.archlinux.de",
    "description": "The German Arch Linux forum",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "archlinux-de/flarum-anti-spam": "^1.1.3",
        "archlinux-de/flarum-click-image": "^1.0.0",
        "archlinux-de/flarum-discussion-feed": "^1.0.0",
        "archlinux-de/flarum-redirect-fluxbb": "^1.4.0",
        "archlinux-de/flarum-redirect-ll": "^1.2.1",
        "archlinux-de/flarum-theme-archlinux": "^1.1.0",
        "flarum-lang/german": "^1.2.2",
        "flarum/bbcode": "^1.2.0",
        "flarum/core": "^1.2.1",
        "flarum/flags": "^1.2.0",
        "flarum/lang-english": "^1.2.0",
        "flarum/likes": "^1.2.0",
        "flarum/lock": "^1.2.0",
        "flarum/markdown": "^1.2.0",
        "flarum/mentions": "^1.2.0",
        "flarum/nicknames": "^1.2.0",
        "flarum/statistics": "^1.2.0",
        "flarum/sticky": "^1.2.0",
        "flarum/subscriptions": "^1.2.0",
        "flarum/suspend": "^1.2.0",
        "flarum/tags": "^1.2.0",
        "fof/spamblock": "^1.0.2",
        "fof/split": "^1.0.1",
        "matteocontrini/flarum-imgur-upload": "^3.9.1",
        "migratetoflarum/old-passwords": "^1.0.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "malukenho/mcbumpface": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "malukenho/mcbumpface": "^1.1.5",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^1.4.10",
        "phpstan/phpstan-deprecation-rules": "^1.0.0",
        "squizlabs/php_codesniffer": "^3.6.2"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-messageformatter": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "extra": {
        "mc-bumpface": {
            "stripVersionPrefixes": true
        }
    }
}
